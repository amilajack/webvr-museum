{
  "name": "webvr-museum",
  "version": "3.0.1",
  "description": "Boilerplate for building virtual reality experiences with A-Frame and React.",
  "dependencies": {
    "aframe": "0.9.2",
    "aframe-ctm-model": "^0.0.2",
    "aframe-extras": "^6.0.0",
    "aframe-orbit-controls": "^1.2.0",
    "aframe-particle-system-component": "^1.0.8",
    "aframe-physics-system": "^3.3.0",
    "aframe-react": "^4.0.x",
    "aframe-room-component": "^0.3.0",
    "aframe-teleport-controls": "^0.3.1",
    "babel-polyfill": "^6.26.0",
    "metmuseum": "^0.0.1-0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom && yarn lint",
    "lint": "eslint .",
    "eject": "react-scripts eject",
    "publish": "npm run build && gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amilajack/webvr-museum.git"
  },
  "keywords": [
    "aframe",
    "react",
    "vr",
    "boilerplate",
    "a-frame",
    "mozvr",
    "reactvr",
    "aframe-react",
    "webvr"
  ],
  "author": "Kevin Ngo <kngo@mozilla.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amilajack/webvr-museum/issues"
  },
  "homepage": "https://amilajack.github.io/webvr-museum",
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-bliss": "^3.4.0",
    "eslint-plugin-react": "^7.13.0",
    "gh-pages": "^2.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:react/recommended",
      "bliss"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "flowtype-errors/show-errors": "off"
    }
  },
  "renovate": {
    "extends": [
      "bliss"
    ]
  }
}
